                        <field name="pragya_type"/>
                        <field name="pragya_no"/>
                    </group>
                    <group>
                        <field name="declaration_fee"/>
                        <field name="vat_fee"/>
                        <!--<field name="duty_fee"/>-->

                    </group>
                </group>
                    <field name="pra_id" >
                        <tree>
                            <field name="bill_no"/>
                            <field name="rate"/>
                            <field name="party_wt"/>
                            <field name="paid_wt"/>
                            <field name="wt_exp"/>
                            <field name="bc_amount"/>
                            <field name="bc_total_amount"/>
                            <field name="veh_no"/>
                            <field name="trans_name"/>
                            <field name="bhada_date"/>
                            <field name="bhada_driver"/>
                            <field name="driver_lic"/>



                        </tree>

                    </field>


                </sheet>
            </form>
        </field>
    </record>


