<odoo>
  <data>
    <!-- uat.test -->
  <act_window id="uat_act-window_id" name ="UAT" res_model="uat.test" view_mode="tree,form"/>
     <menuitem id="uat_menu_id" name="UAT" action="uat_act-window_id"/>
  <act_window id="uat_uat_act-window_id" name ="UAT" res_model="uat.test" view_mode="tree,form"/>
<menuitem id="uat_uat_menu_id" name="UAT" parent='uat_menu_id' action="uat_uat_act-window_id"/>



    

<record id="view_uat_test_tree_id" model="ir.ui.view">
        <field name="name">uat_test_tree</field>
        <field name="model">uat.test</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree>
                <field name="test_date"/>
                <field name="test_project"/>
                <field name="test_version"/>
            </tree>
        </field>
    </record>

    <record id="view_uat_test_form_id" model="ir.ui.view">
        <field name="name">uat_test_form</field>
        <field name="model">uat.test</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <!-- <button string="Send SMS"
                            type="object"
                            name="post_msg"
                            attrs="{'invisible': [('state','!=','draft')]}"/>
                    <field name="state" widget="statusbar" clickable="True"/> -->
                </header>
               <sheet>
                     <h1>
                      UAT
                         <!-- <field name="name" readonly="1"/> -->
                    </h1>
                <group class="well">
                    <group>
                        
                <field name="test_project"/>
                <field name="test_version"/>
                    </group>
                    <group>
                       <field name="test_date"/>

                    </group>
                </group>
                    <field name="test_id">
                        <tree>
                            <field name="statement"/>
                            <field name="expected_result"/>
                            <field name="actual_result"/>

                        </tree>
                    </field>
                </sheet>
            </form>
        </field>
    </record>

